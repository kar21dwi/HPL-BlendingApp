{
  "extends": ["tslint:latest", "tslint-config-prettier"],

  "rules": {
    "member-access": [true, "no-public"],
    "no-console": [
      false,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }

}
